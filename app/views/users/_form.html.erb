<%= form_with model: @user do |form| %>
    <%= form.label :first_name %><br>
    <%= form.text_field :first_name %><br>
    <%= form.label :last_name %><br>
    <%= form.text_field :last_name %><br>
    <%= form.label :email %><br>
    <%= form.text_field :email %><br>
    <%= form.label :phone %><br>
    <%= form.text_field :phone %> <br>
    <%= form.label :social_media %><br>
    <%= form.text_field :social_media%><br>
    <%= form.label :bio %><br>
    <%= form.text_area :bio %><br> 
    <%= form.label :avatar%><br>
    <%= form.file_field :avatar, accept: 'image/png, image/gif, image/jpeg' %> <br><br>
    <%= form.submit %>
<% end %>